{
  "applicationId": "js.neutralino.sample",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "globalVariables": {
    "TEST1": "Hello",
    "TEST2": [
      2,
      4,
      5
    ],
    "TEST3": {
      "value1": 10,
      "value2": {}
    }
  },
  "modes": {
    "window": {
      "title": "neu-translate-tool",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/public/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false,
      "nativeAllowList": [
        "filesystem.writeFile",
        "os.execCommand"
      ]
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    }
  },
  "cli": {
    "binaryName": "neu-translate-tool",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/public/js/neutralino.js",
    "binaryVersion": "4.3.0",
    "clientVersion": "3.2.0"
  }
}